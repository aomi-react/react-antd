{
  "name": "@aomi/react-antd",
  "version": "1.0.0-Alpha.12",
  "main": "index.js",
  "repository": "git@github.com:aomi-react/react-antd.git",
  "author": "sean <sean.snow@live.com>",
  "license": "MIT",
  "scripts": {
    "clean1": "find . -path \"./node_modules\" -prune -o -name \"*.js\" -o -name \"*.js.map\" -o -name \"*.d.ts\" |xargs rm -f | echo ''",
    "clean": "rm -rf es lib",
    "build": "rm -rf es lib types && tsc -d && tsc --project tsconfig.cjs.json",
    "watch": "tsc -d && tsc -w",
    "prepublishOnly": "yarn build"
  },
  "exports": {
    ".": {
      "import": "./es/index.js",
      "types": "./types/index.d.ts",
      "require": "./lib/index.js"
    },
    "./BaseSelect": {
      "import": "./es/BaseSelect/index.js",
      "types": "./types/BaseSelect/index.d.ts",
      "require": "./lib/BaseSelect/index.js"
    },
    "./utils/OptionUtil": {
      "import": "./es/utils/OptionUtil.js",
      "types": "./types/utils/OptionUtil.d.ts",
      "require": "./lib/utils/OptionUtil.js"
    },
    "./utils": {
      "import": "./es/utils/index.js",
      "types": "./types/utils/index.d.ts",
      "require": "./lib/utils/index.js"
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "antd": "^5.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.3"
  }
}
